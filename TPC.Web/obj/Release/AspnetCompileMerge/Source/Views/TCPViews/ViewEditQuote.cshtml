<script>
    $(document).ready(function () {
        $('#divMenus li').removeClass('active navlihide');
        $('#liActiveQuote').addClass('navlihide');
        $('#liEditQuote').addClass('navlihide');
        $('#liListView').addClass('active navlihide');
        $('.navlihide').show();

        $(window).load(LoadEditData(),function () {
        });

        function LoadEditData() {
            $.ajax({
                url: "@Url.Content("~/ItemContainerPartial/ItemContainer")",
                type: "POST",
                data: { quoteID: this.title },
                datatype: 'html',
                success: function (data) {
                    $('#ViewEditQuote').html(data);
                },
                error: function (data) {
                    //alert('error');
                }
            });
        }
    });
</script>
<div id="ViewEditQuote" class="body-text span12">
   
</div>