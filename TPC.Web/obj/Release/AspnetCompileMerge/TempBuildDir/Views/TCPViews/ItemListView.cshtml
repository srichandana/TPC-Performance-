        ViewData["QuoteID"] = Model.QuoteID;
        ViewData["Type"] = Model.KPLItemListVM != null && Model.KPLItemListVM.Count() > 0 ? Model.KPLItemListVM.FirstOrDefault().QuoteFlag: string.Empty;
    }
   
